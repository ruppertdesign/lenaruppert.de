{
  "name": "lenaruppert.de",
  "description": "Lenaruppert website",
  "version": "1.0.0-beta",
  "author": "Florian Hirsch",
  "license": "UNLICENSED",
  "private": true,
  "main": "n/a",
  "scripts": {
    "clean": "rimraf .cache public",
    "develop": "gatsby develop",
    "build": "yarn clean && gatsby build",
    "serve": "gatsby serve",
    "format": "yarn prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.{js,jsx,ts,tsx}'",
    "graphql-types": "gql-gen",
    "type-check": "tsc --noEmit",
    "lint": "yarn tslint --project . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn run lint",
      "yarn run format",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.4",
    "@emotion/styled": "^10.0.4",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.5",
    "bulma": "^0.7.1",
    "emotion": "^10.0.4",
    "emotion-server": "^10.0.4",
    "gatsby": "^2.0.7",
    "gatsby-link": "^2.0.1",
    "gatsby-plugin-emotion": "^4.0.1",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-netlify-cms": "^3.0.2",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.3",
    "gatsby-transformer-sharp": "^2.1.1",
    "graphql-code-generator": "^0.15.2",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms": "^2.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.5.1",
    "react-cookie-consent": "^2.0.0",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-load-script": "^0.0.6",
    "react-markdown": "^4.0.4",
    "react-typography": "^0.16.13",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "typography": "^0.16.17",
    "typography-theme-de-young": "^0.16.9"
  },
  "devDependencies": {
    "graphql-codegen-typescript-template": "^0.15.2",
    "husky": "^1.0.0-rc.15",
    "lint-staged": "^8.0.4",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2"
  }
}
