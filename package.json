{
  "name": "lenaruppert.de",
  "description": "Lenaruppert website",
  "version": "1.0.50",
  "author": "Florian Hirsch",
  "license": "UNLICENSED",
  "private": true,
  "main": "n/a",
  "scripts": {
    "clean": "rimraf .cache public",
    "develop": "gatsby develop",
    "build": "yarn clean && gatsby build",
    "serve": "gatsby serve",
    "format": "yarn prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.{js,jsx,ts,tsx}'",
    "graphql-types": "gql-gen",
    "type-check": "tsc --noEmit",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn run lint",
      "yarn run format"
    ]
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "bulma": "^0.9.4",
    "emotion": "^11.0.0",
    "emotion-server": "^11.0.0",
    "eslint-utils": "^3.0.0",
    "gatsby": "^4.24.4",
    "gatsby-link": "^4.24.1",
    "gatsby-plugin-emotion": "^7.24.0",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^4.24.0",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-netlify-cms": "^6.24.0",
    "gatsby-plugin-react-helmet": "^5.24.0",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-sitemap": "^5.24.0",
    "gatsby-plugin-typescript": "^4.24.0",
    "gatsby-plugin-typography": "^4.24.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^6.24.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-remark": "^5.24.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.4",
    "mixin-deep": "^2.0.1",
    "netlify-cms-app": "^2.15.72",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.0.0",
    "react-load-script": "^0.0.6",
    "react-markdown": "^8.0.3",
    "react-typography": "^0.16.20",
    "set-value": "^4.1.0",
    "typescript": "^4.8.4",
    "typography": "^0.16.21",
    "typography-theme-de-young": "^0.16.19"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-plugin-react": "^7.31.9",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-typescript-client": "^0.18.2",
    "graphql-codegen-typescript-common": "^0.18.2",
    "graphql-codegen-typescript-server": "^0.18.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.0"
  }
}
