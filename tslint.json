{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": {
      "options": [
        "never"
      ]
    },
    "interface-name": [
      "never-prefix"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "no-console": [
      false
    ],
    "no-var-requires": false
  },
  "rulesDirectory": []
}