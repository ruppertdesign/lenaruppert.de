{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "es2017",
      "esnext.asynciterable"
    ],
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmit": true,
    "skipLibCheck": true,
    "types": ["@emotion/react/types/css-prop"]
  },
  "include": [
    "./src/**/*",
    "./typings/**/*"
  ]
}